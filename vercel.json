{
  "version": 2,
  "name": "medcontrol",
  "builds": [
    {
      "src": "frontend-morbilidad/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend-morbilidad/build/$1"
    }
  ]
}
